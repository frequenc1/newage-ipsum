{
    "name": "null/newage-ipsum",
    "type": "library",
    "description": "A new version (compatible to fakerphp) of the new aged ipsum generator forked from frequenc1/newage-ipsum",
    "authors": [
        {
            "name": "Bernhard Strehl"
        }
    ],
    "replace":{
        "frequenc1/newage-ipsum":"dev-master"
    },
    "require": {
        "php": ">=7.1.0",
        "fakerphp/faker": "~1"
    },
    "require-dev": {
        "phpunit/phpunit" : "*"
    },
    "autoload": {
        "psr-4": {
            "NewAgeIpsum\\": "src/"
        }
    }
}